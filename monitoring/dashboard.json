      {
        "title": "GPU Utilization",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"custom.googleapis.com/opencensus/gce/gpu/utilization_distribution\" resource.type=\"gce_instance\"",
